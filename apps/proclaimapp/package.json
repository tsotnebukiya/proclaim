  "name": "proclaimapp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "start": "next start",
    "dev:citi": "env-cmd -f .env.citi next dev -p 3001",
    "dev:jp": "env-cmd -f .env.jp next dev -p 3002",
    "dev:goldman": "env-cmd -f .env.goldman next dev -p 3003",
    "dev": "concurrently \"pnpm run dev:citi\" \"pnpm run dev:jp\" \"pnpm run dev:goldman\"",
    "studio:citi": "env-cmd -f .env.citi prisma studio -p 3011",
    "studio:jp": "env-cmd -f .env.jp prisma studio -p 3012",
    "studio:goldman": "env-cmd -f .env.goldman prisma studio -p 3013",
    "studio": "concurrently \"pnpm run studio:citi\" \"pnpm run studio:jp\" \"pnpm run studio:goldman\"",
    "db:push:citi": "env-cmd -f .env.citi prisma db push",
    "db:push:jp": "env-cmd -f .env.jp prisma db push",
    "db:push:goldman": "env-cmd -f .env.goldman prisma db push",
    "db:push": "pnpm run db:push:citi && pnpm run db:push:jp && pnpm run db:push:goldman",
    "db:reset:citi": "env-cmd -f .env.citi prisma db push --force-reset",
    "db:reset:jp": "env-cmd -f .env.jp prisma db push --force-reset",
    "db:reset:goldman": "env-cmd -f .env.goldman prisma db push --force-reset",
    "db:reset": "pnpm run db:reset:citi && pnpm run db:reset:jp && pnpm run db:reset:goldman",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "start:citi": "env-cmd -f .env.citi next start -p 3001"
  },
